@model IEnumerable<BusinessAdvanceManagement.Domain.DTOs.PageRole.PageRoleListDTO>
<link href="~/css/Dashboard/styles.css" rel="stylesheet" />
@foreach (var item in Model)
{
    <a asp-controller="@item.PageController" asp-action="@item.PageAction" class="list-group-item list-group-item-action bg-transparent second-text active">
        <i class="fas fa-tachometer-alt me-2"></i>@item.PageName
    </a>
}